{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "lib/**/*.{js,jsx}"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "testPathIgnorePatterns": [
    "node_modules",
    "__tests__/helpers.js"
  ],
  "testEnvironment": "node"
}
