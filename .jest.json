{
  "collectCoverageFrom": [
    "**/*.{js}"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "roots": [
    "<rootDir>/lib"
  ],
  "rootDir" : ".",
  "testPathIgnorePatterns": [
    "<rootDir>/(node_modules|test)/"
  ],
  "testRegex": "(\\.(test))\\.(js)$",
  "testEnvironment": "node"
}
